    printf("espace x2");
    scanf("%f",x2);
    printf("aspace,y1");
    scanf("%f",y1);
    printf("espace y2");
    scanf("%f",y2);
     printf("aspace,z1");
    scanf("%f",z1);
    printf("espace z2");
    scanf("%f",z2);
    sqrt=(pow(x2-x1,2) + pow(y2-y1,2) + pow(z2-z1,2));

           return 0;
